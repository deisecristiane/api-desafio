
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Comic {
  id Int @id @default(autoincrement())
  name String
  description String
  price Decimal
  rarity Boolean
  quantity Int
  likes Int
  createdAt DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@map("comics")
}
