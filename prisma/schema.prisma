
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
enum Rarity {
  COMMOM
  RARE
}

model Comic {
  id Int @id @default(autoincrement())
  codComic String @unique
  name String
  description String
  price Decimal
  rarity String @default("COMMOM")
  quantity Int
  likes Int
  createdAt DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@map("comics")
}
